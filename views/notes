          app.post('/moods', (req, res) => {
            let user = req.user._id
            db.collection('entries').insertOne({currentfeeling: req.body.currentfeeling, postedBy: user}, (err, result) => {
              if (err) return console.log(err)
              console.log('saved to database')
              res.redirect('/mood')
            })
          })
          
          
          app.get('/moods', function(req, res) {
            db.collection('entries').find().toArray((err, result) => {
              if (err) return console.log(err)
              res.render('mood.ejs', {
                entries: result,
              })
            })
          });
          
          
